package com.cg.demo;


import java.util.Scanner;

public class LAB11 {

public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	   System.out.println("Please enter an integer - ");
	   int n = sc.nextInt();
	   
	   int i;
	   int sum=0;
	System.out.println("number divisible by 3 & 5");
	   for(i=1;i<=n;i++) {
		   if(i%3==0 || i%5==0)
			   sum+= i;
	   }
	   System.out.println(sum);
}
}
