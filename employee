package com.cg.demo.abs;

// abstract ! = abstraction

// abstraction - minimum necessary representation of a (real time entity)

// you dont create unnecessary details
// real world entity
public class Employee {
	// attributes - fields
	int id;
	String name;
	double salary;

	// functionalities - methods
	void work() {
		System.out.println("Working");
	}

}
